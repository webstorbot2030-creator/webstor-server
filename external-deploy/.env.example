# ===== إعدادات قاعدة البيانات =====
# رابط الاتصال بقاعدة بيانات PostgreSQL
DATABASE_URL=postgresql://username:password@localhost:5432/webstore

# ===== إعدادات الجلسة =====
# مفتاح سري لتشفير الجلسات (غيّره لقيمة عشوائية طويلة)
SESSION_SECRET=your-super-secret-key-change-this

# ===== إعدادات السيرفر =====
# رقم المنفذ (البورت)
PORT=5000

# بيئة التشغيل
NODE_ENV=production
